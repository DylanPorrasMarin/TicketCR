@{
    Layout = "";
}
<!DOCTYPE html>
<html lang="en">

<head>
    <title>Validacion de boleto</title>

    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <script src="https://cdn.jsdelivr.net/npm/instascan@1.0.0/dist/instascan.min.js"></script>

    <!-- Favicon-->
    <link rel="icon" type="image/x-icon" href="assets/favicon.ico" />
    <!-- Bootstrap icons-->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css" rel="stylesheet" />
    <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
    <!-- Core theme CSS (includes Bootstrap)-->
    <link href="~/ticketCR/css/validarBoleto/ValidarBoleto.css" rel="stylesheet" />
    <meta name="viewport" content="width=device-width, initial-scale=1">

</head>
<body>
 
    <div class="main-wrapper">
        <div class="naranja">
            <div class="container">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="image-container-qr">
                                <div id="contenedor-qr"></div>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-md-12">
                            <form id="formulario-validar-boleto">
                                <p>Click al botón para validar codigoQr</p>
                                <div class="contenedor-btn text-center mt-3">
                                    <button type="submit" class="btn-validar">Validar</button>
                                </div>

                            </form>
                        </div>
                    </div>
                </div>
        </div>
    </div>
    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/qrcodejs/1.0.0/qrcode.min.js" integrity="sha512-CNgIRecGo7nphbeZ04Sc13ka07paqdeTu0WR1IM4kNcpmBAUSHSQX0FslNhTDadL4O5SAGapGt4FodqL8My0mA==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    <script src="~/ticketCR/js/validarBoleto/validarBoleto.js"></script>
    <script src="~/ticketCR/js/alertas.js"></script>
    <script src="~/ticketCR/js/site.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>



</body>
</html>
